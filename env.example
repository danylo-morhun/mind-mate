# Mind Mate - Змінні середовища

# Google Apps Script URL (замініть на ваш URL після розгортання)
NEXT_PUBLIC_GOOGLE_APPS_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Google Cloud Project ID
NEXT_PUBLIC_GOOGLE_CLOUD_PROJECT_ID=your-project-id

# Google Cloud Region
NEXT_PUBLIC_GOOGLE_CLOUD_REGION=us-central1

# Google AI API Key (опціонально, для розширених AI функцій)
GOOGLE_AI_API_KEY=your-ai-api-key

# Google AI Model (gemini-1.5-flash, gemini-1.5-pro, gemini-pro)
GOOGLE_AI_MODEL=gemini-1.5-flash

# AI Generation Settings
AI_MAX_TOKENS=1000
AI_TEMPERATURE=0.7
AI_TOP_P=0.9

# Google Natural Language API Key (опціонально)
GOOGLE_NATURAL_LANGUAGE_API_KEY=your-natural-language-api-key

# База даних (Google Sheets ID)
NEXT_PUBLIC_GOOGLE_SHEETS_ID=your-sheets-id

# Налаштування безпеки
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=http://localhost:3000

# Налаштування логування
NODE_ENV=development
LOG_LEVEL=debug

# Налаштування кешування
CACHE_TTL=3600
MAX_CACHE_SIZE=100

# Налаштування обмежень API
MAX_REQUESTS_PER_MINUTE=60
MAX_FILE_SIZE=10485760

# Налаштування аналітики
ANALYTICS_ENABLED=true
ANALYTICS_TRACKING_ID=your-tracking-id
